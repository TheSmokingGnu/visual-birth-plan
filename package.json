{
  "name": "visual-birth-plan",
  "description": "Easy to understand birth plan creator",
  "version": "1.0.0",
  "author": "peter@grainger.xyz",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "start": "node server.js",
    "precommit": "lint-staged",
    "heroku-postbuild": "npm run build",
    "build": "cross-env NODE_ENV=production npm run clean && npm run compile",
    "build:parts": "npm run sass && npm run compile",
    "ci:teamcity": "karma --env=tc start karma.coverage.js && npm run coverage:remap",
    "ci:jenkins": "karma --env=jk start karma.coverage.js && npm run coverage:remap",
    "clean": "rimraf dist && rimraf coverage",
    "compile": "webpack --config config/webpack.config.prod.js",
    "coverage": "npm run coverage:run && npm run coverage:remap && npm run coverage:open",
    "coverage:open": "opn coverage/html-ts/index.html",
    "coverage:remap": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html-ts -t html",
    "coverage:run": "cross-env NODE_ENV=development karma start karma.coverage.js",
    "dev": "webpack-dev-server --hot --inline",
    "lint": "tslint src/**/*.ts",
    "serve": "http-server dist/ -o",
    "test": "cross-env NODE_ENV=development karma start karma.unit.js",
    "test:debug": "cross-env NODE_ENV=development karma start karma.debug.js",
    "test:watch": "cross-env NODE_ENV=development karma start karma.unit.js --singleRun=false --auto-watch"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "~0.18.0",
    "fastify": "^1.2.0",
    "vue": "~2.5.16",
    "vue-class-component": "~6.2.0",
    "vue-property-decorator": "~6.0.0",
    "vue-router": "~3.0.1"
  },
  "devDependencies": {
    "@types/mocha": "~5.0.0",
    "@types/node": "~9.6.1",
    "@types/sinon": "~4.3.0",
    "autoprefixer": "^8.2.0",
    "awesome-typescript-loader": "~4.0.1",
    "bootstrap-sass": "^3.3.7",
    "chai": "~4.1.1",
    "compression-webpack-plugin": "~1.1.11",
    "copy-webpack-plugin": "~4.5.1",
    "cross-env": "~5.1.4",
    "css-loader": "^0.28.5",
    "es6-promise": "~4.2.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.8",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "http-server": "~0.11.1",
    "husky": "^0.14.3",
    "istanbul-instrumenter-loader": "~3.0.0",
    "json-loader": "~0.5.7",
    "karma": "~2.0.0",
    "karma-chai": "~0.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "~1.1.1",
    "karma-junit-reporter": "~1.2.0",
    "karma-mocha": "~1.3.0",
    "karma-mocha-reporter": "~2.2.3",
    "karma-sinon": "~1.0.5",
    "karma-source-map-support": "~1.2.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-teamcity-reporter": "~1.1.0",
    "karma-webpack": "~3.0.0",
    "lint-staged": "^7.0.0",
    "lodash.merge": "~4.6.0",
    "minimist": "~1.2.0",
    "mocha": "~5.0.5",
    "ncp": "~2.0.0",
    "node-sass": "~4.8.3",
    "opn-cli": "~3.1.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.7.4",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "raw-loader": "~0.5.1",
    "remap-istanbul": "~0.11.0",
    "rimraf": "~2.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "~4.4.10",
    "style-loader": "^0.20.3",
    "tslint": "~5.9.1",
    "tslint-config-prettier": "^1.5.0",
    "tslint-loader": "~3.6.0",
    "typescript": "~2.8.1",
    "url-loader": "^1.0.1",
    "webpack": "^3.11.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^2.11.1"
  }
}
